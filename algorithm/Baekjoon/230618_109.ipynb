   "source": [
    "from sys import stdin\n",
    "\n",
    "def f(n, i):\n",
    "    return 1 + f(n//i, i) if n%i == 0 else 0\n",
    "\n",
    "for _ in range(int(stdin.readline())):\n",
    "    N = int(stdin.readline())\n",
    "    cnt = 0\n",
    "    for i in range(2, N+1):\n",
    "        cnt += f(N, i)\n",
    "    print(cnt)"
