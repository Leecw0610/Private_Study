   "source": [
    "n = int(input())\n",
    "data = sorted([int(input()) for _ in range(n)])\n",
    "mn = float('inf')\n",
    "for i in range(n):\n",
    "    cnt = 0\n",
    "    for j in range(data[i],data[i]+5):\n",
    "        if j not in data:\n",
    "            cnt+=1\n",
    "    mn = min(mn,cnt)\n",
    "print(mn)"
   ]
