    "N, M = map(int, input().split())\n",
    "A = list(map(int, input().split()))\n",
    "level = 0\n",
    "result = 0\n",
    "\n",
    "for i in A:\n",
    "    level += i\n",
    "\n",
    "    if level < 0:\n",
    "        level = 0\n",
    "\n",
    "    if level >= M:\n",
    "        result += 1\n",
    "\n",
    "print(result)"
