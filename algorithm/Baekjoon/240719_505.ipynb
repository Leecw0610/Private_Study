    "n = int(input())\n",
    "res = ''\n",
    "t = 0\n",
    "while 9**t <= n:\n",
    "    t += 1\n",
    "res = ''\n",
    "for i in range(t-1, -1, -1):\n",
    "    res += str(n//(9**i))\n",
    "    n = n%(9**i)\n",
    "print(res)"
