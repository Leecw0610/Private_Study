    "n = int(input())\n",
    "\n",
    "for _ in range(n) :\n",
    "  m = input()\n",
    "  data = []\n",
    "\n",
    "  result = 0\n",
    "  for i in range(len(m)) :\n",
    "    if int(m[i]) not in data :\n",
    "      result += 1\n",
    "      data.append(int(m[i]))\n",
    "\n",
    "  print(result)"
