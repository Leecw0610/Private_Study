    "condos = []\n",
    "for i in range(int(input())):\n",
    "    l, p = map(int, input().split())\n",
    "    condos.append([l, p])\n",
    "condos.sort()\n",
    "cost = 10001\n",
    "result = 0\n",
    "for i in condos:\n",
    "    temp = i[1]\n",
    "    if temp < cost:\n",
    "        cost = temp\n",
    "        result += 1\n",
    "print(result)"
