   "source": [
    "import sys\n",
    "input = sys.stdin.readline\n",
    "\n",
    "N = int(input())\n",
    "lst = list(map(int, input().split()))\n",
    "\n",
    "res = 0\n",
    "max_res = 0\n",
    "for i in lst:\n",
    "    if i == 0:\n",
    "        res = 0\n",
    "        continue\n",
    "    res += 1\n",
    "    max_res = max(res, max_res)\n",
    "print(max_res)"
