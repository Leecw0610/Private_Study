    "for _ in range(int(input())):\n",
    "    L, D = input().split('-')\n",
    "    n = int(D)\n",
    "    s = 0\n",
    "    for i in range(3):\n",
    "        s += (ord(L[i])-65) * 26**(2-i)\n",
    "    print(\"nice\" if abs(s-n) <= 100 else \"not nice\")"
   ]
