    "k = int(input())\n",
    "\n",
    "for _ in range(k) :\n",
    "  p, m = map(int, input().split())\n",
    "  data = [0] * (m + 1)\n",
    "  count = 0\n",
    "\n",
    "  for _ in range(p) :\n",
    "    value = int(input())\n",
    "    if data[value] == 0 :\n",
    "      data[value] = 1\n",
    "    else :\n",
    "      count += 1\n",
    "\n",
    "  print(count)"
