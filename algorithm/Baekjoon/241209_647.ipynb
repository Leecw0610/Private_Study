    "n = int(input())\n",
    "arr = [i+1 for i in range(n)]\n",
    "tmp = []\n",
    "while len(arr) != 1:\n",
    "    for idx, item in enumerate(arr):\n",
    "        if idx % 2:\n",
    "            tmp.append(item)\n",
    "    arr = tmp\n",
    "    tmp = []\n",
    "print(arr[0])"
