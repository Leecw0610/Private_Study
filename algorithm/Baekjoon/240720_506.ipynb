   "source": [
    "N = int(input())\n",
    "cnt = 0\n",
    "for i in range(1, (N-2)//2+1):\n",
    "    for j in range(2, N-i+1):\n",
    "        K = N - (2*i + j)\n",
    "        if K > 0 and K%2 == 0:\n",
    "            cnt += 1\n",
    "print(cnt)"
   ]
