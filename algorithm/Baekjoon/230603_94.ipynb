    "li = []\n",
    "for _ in range(int(input())):\n",
    "    li.append(int(input()))\n",
    "\n",
    "ans = li[-1]\n",
    "if li[2]-li[1] == li[1]-li[0]:\n",
    "    ans += (li[1]-li[0])\n",
    "elif li[2]//li[1] == li[1]//li[0]:\n",
    "    ans *= (li[1]//li[0])\n",
    "\n",
    "print(ans)"
