    "import sys\n",
    "input = sys.stdin.readline\n",
    "import re\n",
    "\n",
    "s = input().strip()\n",
    "k = input().strip()\n",
    "\n",
    "new = re.sub(r'[0-9]+', '', s)\n",
    "\n",
    "if k in new:\n",
    "    print(1)\n",
    "else:\n",
    "    print(0)"
