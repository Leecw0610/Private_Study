   "source": [
    "cocktail = list(map(int, input().split()))\n",
    "check = False\n",
    "result = 1\n",
    "\n",
    "for i in range(3) :\n",
    "    if cocktail[i] % 2 != 0 :   # 홀수 판별\n",
    "        result *= cocktail[i]\n",
    "        check = True\n",
    "\n",
    "if check :\n",
    "    print(result)\n",
    "else :\n",
    "    print(cocktail[0] * cocktail[1] * cocktail[2])"
   ]
