   "source": [
    "N = int(input())\n",
    "cnt = 0\n",
    "for i in range(1, N):\n",
    "    if i ** 2 - (i - 1) ** 2 > N:\n",
    "        break\n",
    "    for j in range(1, i + 1):\n",
    "        if i ** 2 - j ** 2 == N:\n",
    "            cnt += 1\n",
    "print(cnt)"
