   "source": [
    "n = int(input())\n",
    "customer = list(map(int, input().split()))\n",
    "cnt = 0\n",
    "seat = []\n",
    "for i in range(n):\n",
    "    if customer[i] in seat:\n",
    "        cnt += 1\n",
    "    else:\n",
    "        seat.append(customer[i])\n",
    "print(cnt)"
