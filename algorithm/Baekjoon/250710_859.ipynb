    "N = int(input())\n",
    "slogan = input()\n",
    "separator = [\"|\", \":\", \"#\"]\n",
    "\n",
    "for s in separator:\n",
    "    slogan = slogan.replace(s, \".\")\n",
    "\n",
    "numberList = slogan.split(\".\")\n",
    "result = 0\n",
    "\n",
    "for n in numberList:\n",
    "    result += int(n)\n",
    "\n",
    "print(result)"
