   "source": [
    "x = int(input())\n",
    "\n",
    "stick = [64, 32, 16, 8, 4, 2, 1]\n",
    "count = 0\n",
    "\n",
    "for i in range(len(stick)):\n",
    "    if x >= stick[i]:\n",
    "        count += 1\n",
    "        x -= stick[i]\n",
    "\n",
    "    if x == 0:\n",
    "        break\n",
    "\n",
    "print(count)"
   ]
