   "source": [
    "def lcm(a, b):\n",
    "    return int((a * b) / gcd(a, b))\n",
    "\n",
    "def gcd(a, b):\n",
    "    if a == 0:\n",
    "        return b\n",
    "    else:\n",
    "        return gcd(b % a, a)\n",
    "\n",
    "t = int(input())\n",
    "for i in range(t):\n",
    "    a, b = map(int, input().split())\n",
    "    print(lcm(a, b), gcd(a, b))"
   ]
