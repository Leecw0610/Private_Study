    "t = int(input())\n",
    "\n",
    "for _ in range(t) :\n",
    "    data = [0] * 27\n",
    "    value = input()\n",
    "    for i in range(len(value)) :\n",
    "        data[int(ord(value[i]))-65] += 1\n",
    "\n",
    "    result = 0\n",
    "    for i in range(26) :\n",
    "        if data[i] == 0 :\n",
    "            result += i + 65\n",
    "\n",
    "    print(result)"
