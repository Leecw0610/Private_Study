   "source": [
    "import sys\n",
    "n = int(sys.stdin.readline())\n",
    "nums = sorted(list(map(int, sys.stdin.readline().split())))\n",
    "is_full = 1\n",
    "\n",
    "for i in range(len(nums)):\n",
    "    if (i + 1) != nums[i]:\n",
    "        print(i + 1)\n",
    "        is_full = 0\n",
    "        break\n",
    "if is_full == 1:\n",
    "    print(n + 1)\n"
   ]
