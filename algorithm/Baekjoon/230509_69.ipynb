   "source": [
    "n = int(input())\n",
    "s = list(map(int, input().split()))\n",
    "a = int(input())\n",
    "sum = 0\n",
    "for i in s:\n",
    "    if i % a == 0:\n",
    "        sum += i // a\n",
    "    else:\n",
    "        sum += i // a + 1\n",
    "print(sum * a)"
   ]
