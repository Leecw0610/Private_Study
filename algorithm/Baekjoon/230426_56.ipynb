   "source": [
    "n, m = map(int,input().split())\n",
    "board = []\n",
    "\n",
    "for _ in range(n):\n",
    "    board.append(input())\n",
    "\n",
    "a, b = 0, 0\n",
    "\n",
    "for i in range(n):\n",
    "    if \"X\" not in board[i]:\n",
    "        a += 1\n",
    "\n",
    "for j in range(m):\n",
    "    if \"X\" not in [board[i][j] for i in range(n)]:\n",
    "        b += 1\n",
    "\n",
    "print(max(a, b))"
