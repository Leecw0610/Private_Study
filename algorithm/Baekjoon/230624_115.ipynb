    "n = int(input())\n",
    "s = \"%.250f\" % (2 ** (-n))\n",
    "last = len(s)\n",
    "for i in range(last - 1, 1, -1):\n",
    "    if s[i] != '0':\n",
    "        last = i\n",
    "        break\n",
    "print(s[:last + 1])"
