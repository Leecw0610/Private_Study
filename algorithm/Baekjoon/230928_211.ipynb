    "import sys\n",
    "input = sys.stdin.readline\n",
    "\n",
    "a = list(map(int, input().split()))\n",
    "a.sort()\n",
    "if a[1] - a[0] == a[2] - a[1]:\n",
    "    print(a[2] * 2 - a[1])\n",
    "elif a[1] - a[0] > a[2] - a[1]:\n",
    "    print(a[1] * 2 - a[2])\n",
    "else:\n",
    "    print(a[1] * 2 - a[0])"
