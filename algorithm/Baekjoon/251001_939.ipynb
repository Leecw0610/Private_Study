   "source": [
    "n, l = map(int, input().split())\n",
    "\n",
    "rocate = list(map(int, input().split()))\n",
    "\n",
    "rocate.sort()\n",
    "\n",
    "tmp = rocate[0]\n",
    "answer = 1\n",
    "for i in range(1, n):\n",
    "    if rocate[i] - tmp >= l:\n",
    "        answer += 1\n",
    "        tmp = rocate[i]\n",
    "print(answer)"
