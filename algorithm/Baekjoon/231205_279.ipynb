   ],
   "source": [
    "for _ in range(int(input())):\n",
    "    n = int(input())\n",
    "    start = 1\n",
    "    print(\"Pairs for %d:\" %n, end = ' ')\n",
    "\n",
    "    for k in range((n-1)//2):\n",
    "        if k != 0:\n",
    "            print(',', end = ' ')\n",
    "        print(start, n - start, end = '')\n",
    "        start += 1\n",
    "\n",
    "    print()"
