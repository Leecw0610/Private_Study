   ],
   "source": [
    "n = int(input())\n",
    "number = int(input())\n",
    "\n",
    "while number:\n",
    "    if number % n == 0:\n",
    "        print(\"%s is a multiple of %s.\" % (number, n))\n",
    "    else:\n",
    "        print(\"%s is NOT a multiple of %s.\" % (number, n))\n",
    "    number = int(input())"
