    "from math import ceil\n",
    "\n",
    "N = int(input())\n",
    "li = list(map(int, input().split()))\n",
    "B, C = map(int, input().split())\n",
    "res = 0\n",
    "for n in li:\n",
    "    n -= B\n",
    "    res += 1\n",
    "    if n > 0:\n",
    "        res += ceil(n/C)\n",
    "print(res)"
