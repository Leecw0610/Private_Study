    "M = int(input())\n",
    "cups = [1, 2, 3]\n",
    "for _ in range(M):\n",
    "    x, y = map(int, input().split())\n",
    "    c1 = cups.index(x)\n",
    "    c2 = cups.index(y)\n",
    "    cups[c1], cups[c2] = cups[c2], cups[c1]\n",
    "\n",
    "print(cups[0])"
