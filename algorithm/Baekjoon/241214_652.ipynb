    "import sys\n",
    "N,C = map(int,input().split())\n",
    "b,a = N,N\n",
    "for _ in range(C):\n",
    "    y,x = map(int,input().split())\n",
    "    if(0<=x<=a and 0<=y<=b):\n",
    "        if(x*b>y*a):\n",
    "            a=x\n",
    "        else:\n",
    "            b=y\n",
    "    else:\n",
    "        pass\n",
    "print(a*b)"
