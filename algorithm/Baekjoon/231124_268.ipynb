    "i = 0\n",
    "while True:\n",
    "    i += 1\n",
    "    l, p, v = map(int, input().split())\n",
    "    if l == 0 and p == 0 and v == 0:\n",
    "        break\n",
    "    a = v // p\n",
    "    b = v % p\n",
    "    if l < b:\n",
    "        b = l\n",
    "    print(\"Case %d: %d\" %(i, a * l + b))\n"
