    "import sys\n",
    "li=[i+1 for i in range(20)]\n",
    "\n",
    "for i in range(10):\n",
    "    m, n = map(int, sys.stdin.readline().split())\n",
    "    a=li[:m-1]\n",
    "    b=li[m-1:n][::-1]\n",
    "    c=li[n:]\n",
    "    li=a+b+c\n",
    "\n",
    "for i in li:\n",
    "    print(i, end=' ')"
