    "n = int(input())\n",
    "for i in range(n):\n",
    "    num = int(input())\n",
    "    cal = 10\n",
    "    while num > cal:\n",
    "        if num % cal >= cal / 2:\n",
    "            num = ((num // cal) + 1) * cal\n",
    "        else:\n",
    "            num = (num // cal) * cal\n",
    "        cal *= 10\n",
    "    print(num)"
