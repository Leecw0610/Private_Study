   "source": [
    "N = int(input())\n",
    "max_T, min_B = 1, 5000\n",
    "\n",
    "for _ in range(N):\n",
    "    T, B = map(int, input().split())\n",
    "    max_T = max(max_T, T)\n",
    "    min_B = min(min_B, B)\n",
    "\n",
    "print((max_T * min_B) % 7 + 1)"
   ]
