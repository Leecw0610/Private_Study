    "import math\n",
    "\n",
    "print('n e\\n- -----------')\n",
    "for n in range(10):\n",
    "    s = str(sum(map(lambda i: 1 / math.factorial(i), range(n + 1)))).split('.')\n",
    "    print(n, f'{float(\".\".join(s)):0.9f}' if len(s[1]) > 9 else \".\".join(s) if s[1] != '0'else s[0])"
