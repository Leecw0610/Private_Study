    "N, L, W, H = map(int, input().split())\n",
    "s, e = 0, max(L, W, H)\n",
    "for _ in range(100):\n",
    "    m = (s+e)/2\n",
    "    if (L//m)*(W//m)*(H//m) >= N:\n",
    "        s = m\n",
    "    else:\n",
    "        e = m\n",
    "print(\"%.10f\" %(e))"
