    "N, K, P = map(int, input().split())\n",
    "bread = list(map(int, input().split()))\n",
    "bundle = [bread[i * K : (i + 1) * K] for i in range((len(bread) + K - 1) // K)]\n",
    "\n",
    "result = N\n",
    "for b in bundle:\n",
    "    if b.count(0) >= P:\n",
    "        result -= 1\n",
    "\n",
    "print(result)"
   ]
