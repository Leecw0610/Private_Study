   "source": [
    "from sys import stdin\n",
    "\n",
    "for _ in range(int(stdin.readline())):\n",
    "    n, m = map(int, stdin.readline().split())\n",
    "    cnt = 0\n",
    "    for a in range(1, n-1):\n",
    "        for b in range(a+1, n):\n",
    "            if (a**2 + b**2 +m)%(a*b) == 0:\n",
    "                cnt += 1\n",
    "    print(cnt)"
   ]
