    "T = int(input())\n",
    "\n",
    "for _ in range(T):\n",
    "    s = input().replace(' ', '')\n",
    "    count_s = [0] * 26\n",
    "\n",
    "    for i in s:\n",
    "        count_s[ord(i) - 97] += 1\n",
    "\n",
    "    if count_s.count(max(count_s)) > 1:\n",
    "        print('?')\n",
    "    else:\n",
    "        print(chr(97 + count_s.index(max(count_s))))\n"
