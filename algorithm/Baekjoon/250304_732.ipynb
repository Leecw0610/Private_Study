    "import sys\n",
    "import math\n",
    "input = sys.stdin.readline\n",
    "\n",
    "n = int(input())\n",
    "\n",
    "if n == 0:\n",
    "    print(0)\n",
    "elif n == 1:\n",
    "    print(1)\n",
    "else:\n",
    "    res = math.ceil(round(math.log(n, 2), 10))\n",
    "    print(res+1)"
