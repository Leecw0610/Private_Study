   "source": [
    "a, b, c, n = map(int, input().split())\n",
    "res = 0\n",
    "for i in range(0, n+1, a):\n",
    "    for j in range(0, n-i+1, b):\n",
    "        for k in range(0, n-i-j+1, c):\n",
    "            if i+j+k == n:\n",
    "                res = 1\n",
    "print(res)"
