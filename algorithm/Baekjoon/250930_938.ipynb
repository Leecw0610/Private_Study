   "source": [
    "import sys\n",
    "\n",
    "n = int(sys.stdin.readline())\n",
    "m = sorted([int(sys.stdin.readline()) for _ in range(n)], reverse=True)\n",
    "answer = -1\n",
    "\n",
    "for i in range(n - 2):\n",
    "    if m[i] < m[i + 1] + m[i + 2]:\n",
    "        answer = m[i] + m[i + 1] + m[i + 2]\n",
    "        break\n",
    "print(answer)"
