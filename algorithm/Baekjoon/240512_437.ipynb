    "import sys\n",
    "n, m = map(int,sys.stdin.readline().split())\n",
    "cnt = []\n",
    "for i in range(m):\n",
    "    a, b = map(int,sys.stdin.readline().split())\n",
    "    cnt.append(a)\n",
    "    cnt.append(b)\n",
    "\n",
    "for i in range(1, n + 1):\n",
    "    print(cnt.count(i))"
