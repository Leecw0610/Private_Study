   "source": [
    "while 1:\n",
    "    M, A, B = map(int, input().split())\n",
    "    if M == A == B == 0:\n",
    "        break\n",
    "    t = round((M/A - M/B)*3600)\n",
    "    h = t//3600\n",
    "    m = (t%3600) // 60\n",
    "    s = t%60\n",
    "    print(\"%d:%02d:%02d\" % (h, m, s))"
   ]
