    "h1, m1, s1 = map(int, input().split(':'))\n",
    "h2, m2, s2 = map(int, input().split(':'))\n",
    "\n",
    "t = (h2 * 3600) + (m2 * 60) + s2 - ((h1 * 3600) + (m1 * 60) + s1)\n",
    "if t < 0:\n",
    "    t += 60 * 60 * 24\n",
    "h = t // 3600\n",
    "m = (t % 3600) // 60\n",
    "s = t % 60\n",
    "print(\"%02d:%02d:%02d\" % (h, m, s))"
   ]
