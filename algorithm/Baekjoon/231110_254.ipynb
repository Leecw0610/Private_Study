   "source": [
    "while 1:\n",
    "    n = int(input())\n",
    "    if n == -1:\n",
    "        break\n",
    "\n",
    "    cp = res = 0\n",
    "    for _ in range(n):\n",
    "        s, t = map(int, input().split())\n",
    "        res += s * (t - cp)\n",
    "        cp = t\n",
    "    print(f\"{res} miles\")"
   ]
