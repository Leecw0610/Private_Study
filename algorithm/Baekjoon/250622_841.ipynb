   "source": [
    "N, A, B = map(int, input().split())\n",
    "x, y = 1, 1\n",
    "\n",
    "while N:\n",
    "    # 1번 조건\n",
    "    x += A\n",
    "    y += B\n",
    "\n",
    "    # 2번 조건\n",
    "    if x < y:\n",
    "        x, y = y, x\n",
    "\n",
    "    # 3번 조건\n",
    "    if x == y:\n",
    "        y -= 1\n",
    "\n",
    "    N -= 1\n",
    "\n",
    "print(x, y)"
   ]
