   "source": [
    "N = int(input())\n",
    "li = [1]*230\n",
    "for i in range(2, int(230**0.5)+1):\n",
    "    if li[i] == 1:\n",
    "        for j in range(i+i, 230, i):\n",
    "            li[j] = 0\n",
    "prime = [i for i in range(2, 230) if li[i] == 1]\n",
    "res = [1] + prime[:N-1]\n",
    "print(N)\n",
    "print(*res)"
   ]
