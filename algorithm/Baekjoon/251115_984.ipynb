    "N = int(input())\n",
    "n = 9\n",
    "a = 1\n",
    "ans = 0\n",
    "\n",
    "while(N != 0):\n",
    "    if N - n > 0:\n",
    "        ans += a*n\n",
    "        N -= n\n",
    "        n = n*10\n",
    "        a += 1\n",
    "    else:\n",
    "        ans += a*N\n",
    "        break\n",
    "\n",
    "print(ans)"
   ]
