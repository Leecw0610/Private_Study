   "source": [
    "N = int(input())\n",
    "li = list(map(int, input().split()))\n",
    "res = []\n",
    "for i in range(N-1):\n",
    "    cnt = 0\n",
    "    for j in range(i+1, N):\n",
    "        if li[i] > li[j]:\n",
    "            cnt += 1\n",
    "        else:\n",
    "            break\n",
    "    res.append(cnt)\n",
    "print(max(res))"
   ]
