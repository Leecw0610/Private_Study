   "source": [
    "import sys\n",
    "input = sys.stdin.readline\n",
    "\n",
    "n = int(input())\n",
    "f = int(input())\n",
    "ans = f\n",
    "for _ in range(n):\n",
    "    i, j = map(int, input().split())\n",
    "    f += (i-j)\n",
    "    if f < 0: ans = 0;break\n",
    "    ans = max(ans, f)\n",
    "print(ans)"
   ]
