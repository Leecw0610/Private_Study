   "source": [
    "import sys\n",
    "input = sys.stdin.readline\n",
    "\n",
    "N, X, K = map(int, input().split())\n",
    "cup = [0] * (N+1)\n",
    "cup[X] = 1\n",
    "for _ in range(K):\n",
    "    a, b = map(int, input().split())\n",
    "    cup[a], cup[b] = cup[b], cup[a]\n",
    "\n",
    "for idx, i in enumerate(cup):\n",
    "    if i == 1:\n",
    "        print(idx)"
   ]
