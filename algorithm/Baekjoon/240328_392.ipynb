   "source": [
    "import sys\n",
    "input = sys.stdin.readline\n",
    "\n",
    "N = int(input())\n",
    "files = [input().rstrip() for _ in range(N)]\n",
    "\n",
    "for i in range(N-1):\n",
    "    for j in range(i,N):\n",
    "        if files[i][::-1] == files[j]:\n",
    "            print(len(files[i]), files[i][len(files[i])//2])\n",
    "            exit()"
   ]
