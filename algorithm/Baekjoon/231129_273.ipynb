    "while True:\n",
    "    try:\n",
    "        line = input()\n",
    "        nmb = list(map(float, line.split(\" \")))\n",
    "        cnt = 0\n",
    "\n",
    "        while nmb[2] > nmb[0]:\n",
    "            nmb[0] += nmb[0] * nmb[1] / 100\n",
    "            cnt += 1\n",
    "\n",
    "        print(cnt)\n",
    "    except:\n",
    "        break"
