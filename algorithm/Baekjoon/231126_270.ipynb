    "    a, b, c = map(int, input().split())\n",
    "    if a == b == c == 0:\n",
    "        break\n",
    "    if b - a == c - b:\n",
    "        print(f\"AP {c + c - b}\")\n",
    "    else:\n",
    "        print(f\"GP {c * (c // b)}\")"
