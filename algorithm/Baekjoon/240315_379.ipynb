    "while 1:\n",
    "    t = list(map(int, input().split()))\n",
    "    if t == [0]:\n",
    "        break\n",
    "    for i in range(t[0]//4):\n",
    "        li = [2*i+1, 2*i+2, t[0]-2*i-1, t[0]-2*i]\n",
    "        if t[1] in li:\n",
    "            li.remove(t[1])\n",
    "            print(*li)"
