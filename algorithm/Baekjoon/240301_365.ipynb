    "a, b = input().split()\n",
    "la, lb = len(a), len(b)\n",
    "if la > lb:\n",
    "    b = '0'*(la-lb) + b\n",
    "else:\n",
    "    a = '0'*(lb-la) + a\n",
    "res = \"\"\n",
    "for i in range(len(a)):\n",
    "    res += str(int(a[i]) + int(b[i]))\n",
    "print(res)"
