   "source": [
    "import sys\n",
    "input = sys.stdin.readline\n",
    "\n",
    "N = int(input())\n",
    "li = [int(input()) for _ in range(N)]\n",
    "stack, cnt = [li.pop()], 1\n",
    "for n in li[::-1]:\n",
    "    if stack[-1] < n:\n",
    "        cnt += 1\n",
    "        stack.append(n)\n",
    "print(cnt)"
   ]
