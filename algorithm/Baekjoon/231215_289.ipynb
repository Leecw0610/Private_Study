   "source": [
    "from sys import stdin\n",
    "\n",
    "a = b = 0\n",
    "for _ in range(int(stdin.readline())):\n",
    "    A, B = map(int, stdin.readline().split())\n",
    "    if A > B:\n",
    "        a += 1\n",
    "    elif A < B:\n",
    "        b += 1\n",
    "print(a, b)"
   ]
