    "import math\n",
    "N = int(input())\n",
    "List = list(map(int,input().split()))\n",
    "a = 0\n",
    "b = 0\n",
    "\n",
    "for i in range(len(List)):\n",
    "    if(List[i]%2 != 0):\n",
    "        a += 1\n",
    "    else:\n",
    "        b += 1\n",
    "if(a == math.ceil(N/2) and b == N//2):\n",
    "    print(1)\n",
    "else:\n",
    "    print(0)"
