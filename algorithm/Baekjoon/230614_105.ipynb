    "day = 0\n",
    "month = [31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31]\n",
    "week = [\"SUN\",\"MON\",\"TUE\",\"WED\",\"THU\",\"FRI\",\"SAT\"]\n",
    "\n",
    "x, y = map(int, input().split())\n",
    "\n",
    "for i in range(x - 1):\n",
    "    day += month[i]\n",
    "\n",
    "day = (day + y) % 7\n",
    "\n",
    "print(week[day])"
