   "source": [
    "N = int(input())\n",
    "cards = list(map(int, input().split())) + [0] \n",
    "result = []\n",
    "tmp = []\n",
    "\n",
    "for i in range(N):\n",
    "    tmp.append(cards[i])\n",
    "    if (cards[i+1] - cards[i]) != 1:\n",
    "        result.append(tmp)\n",
    "        tmp = []\n",
    "total = 0\n",
    "for j in result:\n",
    "    total += j[0]\n",
    "print(total)"
