    "while True:\n",
    "    li = list(map(int, input().split()))\n",
    "    leaf = 1\n",
    "    if li[0] == 0:\n",
    "        break\n",
    "\n",
    "    for i in range(1, len(li), 2):\n",
    "        leaf = leaf * li[i] - li[i + 1]\n",
    "    print(leaf)"
