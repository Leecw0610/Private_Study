    "case = int(input())\n",
    "\n",
    "for _ in range(case):\n",
    "    r, e, c = map(int, input().split())\n",
    "    if r > e - c:\n",
    "        print('do not advertise')\n",
    "    elif r == e - c:\n",
    "        print('does not matter')\n",
    "    else:\n",
    "        print('advertise')"
