   "source": [
    "a, b = input().split()\n",
    "\n",
    "answer = []\n",
    "for i in range(len(b) - len(a) + 1):\n",
    "    count = 0\n",
    "    for j in range(len(a)):\n",
    "        if a[j] != b[i + j]:\n",
    "            count += 1\n",
    "    answer.append(count)\n",
    "\n",
    "print(min(answer))"
   ]
