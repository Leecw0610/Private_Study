    "\n",
    "for i in range(n):\n",
    "    points = list(map(int, input().split()))\n",
    "    points.remove(max(points))\n",
    "    points.remove(min(points))\n",
    "    if max(points) - min(points) >= 4:\n",
    "        print('KIN')\n",
    "    else:\n",
    "        print(sum(points))"
