   "source": [
    "L, R = map(str, input().split())\n",
    "count = 0\n",
    "\n",
    "if L == R:\n",
    "    count = str(L).count('8')\n",
    "elif len(L) == len(R):\n",
    "    for i in range(len(L)):\n",
    "        if L[i] == '8' and R[i] == '8':\n",
    "            count += 1\n",
    "        elif L[i] != R[i]:\n",
    "            break\n",
    "\n",
    "print(count)"
