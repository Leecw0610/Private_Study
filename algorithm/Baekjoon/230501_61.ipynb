   "source": [
    "N = int(input())\n",
    "fee = list(map(int, input().split()))\n",
    "y, m = 0, 0\n",
    "for n in fee:\n",
    "    y += (n//30 + 1) * 10\n",
    "    m += (n//60 + 1) * 15\n",
    "if m == y:\n",
    "    print(\"Y M\", m)\n",
    "elif m < y:\n",
    "    print(\"M\", m)\n",
    "else:\n",
    "    print(\"Y\", y)"
