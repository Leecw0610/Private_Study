    "from math import pi\n",
    "\n",
    "i = 1\n",
    "while 1:\n",
    "    d, r, t = map(float, input().split())\n",
    "    if r == 0:\n",
    "        break\n",
    "    dis = d / 63360 * pi * r\n",
    "    mph = dis / t * 3600\n",
    "    print(\"Trip #%d: %.2f %.2f\" % (i, dis, mph))\n",
    "    i += 1"
   ]
