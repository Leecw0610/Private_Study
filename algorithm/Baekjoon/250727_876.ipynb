   "source": [
    "import sys\n",
    "input = sys.stdin.readline\n",
    "\n",
    "# 입력\n",
    "N, a, b = map(int, input().split())\n",
    "\n",
    "# 토너먼트\n",
    "cnt = 0\n",
    "while a != b:\n",
    "    a -= a // 2\n",
    "    b -= b // 2\n",
    "    cnt += 1\n",
    "print(cnt)"
   ]
