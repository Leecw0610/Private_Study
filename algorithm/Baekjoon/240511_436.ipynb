   "source": [
    "import sys\n",
    "input = sys.stdin.readline\n",
    "r_d = 0\n",
    "r_n = 1\n",
    "\n",
    "for i in range(int(input())):\n",
    "    num = list(map(int,input().split()))\n",
    "    r_n = int(r_n * num[1]/num[0])\n",
    "    r_d = (r_d + num[2]) % 2\n",
    "\n",
    "print(r_d, r_n)"
   ]
